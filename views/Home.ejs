<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <title>Object Demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <!-- <link rel="stylesheet" href="styles.css"> -->
    <script src = "https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"> </script>
</head>
  <body style="background-color: rgb(197, 53, 53)">
    <nav class="navbar navbar-expand-lg p-3" style="background-color: rgb(174, 21, 21);">
      <div class="container-fluid">
          <a class="nav-link dropdown-toggle text-light" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
          </a>
          <ul class="dropdown-menu">
            <li><a class="dropdown-item" href="Home.ejs">Home</a></li>
            <li><a class="dropdown-item" href="Create.ejs">Create</a></li>
          </ul>
      </div>
    </nav>
    <div class="m-5 p-5">
        <div class="row">
            <div class="col-4"></div>
            <div class="col-4 text-white"><h1 class="text-center">Drinks</h1></div>
            <div class="col-4"></div>
        </div>
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Subject</th>
                <th scope="col">Section</th>
                <th scope="col">Assessment Name</th>
                <th scope="col">Quiz or Test</th>
                <th scope="col">Date to Make up</th>
                <th scope="col">Period to Make up</th>
                <th scope="col">Delete</th>
                <th scope="col">Update</th>
              </tr>
            </thead>
            <tbody id="carsTable">
             <%for(var i=0; i < data.length; i++){%>
                <tr>
                  <th scope="row"><%=i+1%></th>
                  <td><%= data[i].Name %></td>
                  <td><%= data[i].Subject %></td>
                  <td><%= data[i].Section %></td>
                  <td><%= data[i].Assessment %></td>
                  <td><%= data[i].Quiz_or_Test %></td>
                  <td><%= data[i].Date_to_Make_up %></td>
                  <td><%= data[i].Period_to_Make_up %></td>
                    <td><a href="/delete/<%= i %>" class="btn btn-danger">Delete</a></td>
                    <td><a href="/update/<%= i %>" class="btn btn-warning">Update</a></td>
                </tr>
             <% } %>
            </tbody>
          </table>
    </div>
    <script type="text/javascript" src="app.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>